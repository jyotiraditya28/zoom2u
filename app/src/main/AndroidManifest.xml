<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zoom2u_customer">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".utility.Zoom2u"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Zoom2u">

        <activity android:name=".application.ui.details_base_page.profile.my_location.search_location.Location" />
        <activity
            android:name=".application.splash_screen.LogInSignupMainActivity"
            android:theme="@style/AppTheme.Splash" />
        <activity android:name=".application.ui.details_base_page.BottomNavigationActivity" />
        <activity android:name=".application.ui.details_base_page.profile.edit_profile.EditProfileActivity" />
        <activity android:name=".application.splash_screen.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".application.ui.details_base_page.profile.chnage_password.ChangePassActivity" />
        <activity android:name=".application.ui.log_in.forgot_password.ForgotPasswordActivity" />
        <activity android:name="com.zoom2u_customer.application.ui.details_base_page.home.DocDimensionActivity" />
        <activity android:name=".application.ui.details_base_page.profile.my_location.edit_add_location.EditAddLocationActivity" />
        <activity android:name=".application.ui.details_base_page.profile.my_location.MyLocationActivity" />
        <activity android:name=".application.ui.details_base_page.profile.change_price.PriceChangeActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_api_key" />

        <activity android:name=".application.ui.bid_request.active_bid_request.active_bid_page.ActiveBidActivity" />
        <activity android:name=".application.ui.bid_request.BidRequestActivity" />
        <activity android:name=".application.ui.qutote_request.QuoteRequestActivity" /> <!-- <activity android:name=".application.ui.chat.MessageActivity" /> -->
        <activity android:name=".application.ui.chat.ChatActivity" />
        <activity android:name=".application.ui.details_base_page.history.history_details.HistoryDetailsActivity" />
        <activity android:name=".application.ui.log_in.LogInActivity" />
        <activity android:name=".application.ui.sign_up.SignUpActivity" />
        <activity android:name=".application.ui.details_base_page.home.pricing_payment.PricingPaymentActivity" />
        <activity android:name=".application.ui.details_base_page.home.map_page.MapActivity" />
        <activity android:name=".application.ui.details_base_page.home.delivery_details.DeliveryDetailsActivity" />
        <activity android:name=".application.ui.details_base_page.base_page.BasePageActivity" />
    </application>

</manifest>