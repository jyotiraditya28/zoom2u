<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">


        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_margin="10dp"
            android:src="@drawable/ic_back_grey"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/header_txt"
            style="@style/style_15_bold"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_margin="10dp"
            android:text="@string/delivery_details"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/chat_btn"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_margin="10dp"
            android:src="@drawable/ic_chat_grey"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <fragment
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="270dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/header_txt" />

        <ScrollView
            android:id="@+id/scrollView"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="190dp"
            android:layout_weight="1"
            android:scrollbars="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/header_txt">


            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/card"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="4dp"
                    android:background="@drawable/white_background"
                    android:elevation="5dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/document_icon"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/ic_documents"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/document_txt"
                        style="@style/style_13_bold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="20dp"
                        android:text="@string/document"
                        android:textColor="@color/black"
                        app:layout_constraintStart_toEndOf="@+id/document_icon"
                        app:layout_constraintTop_toTopOf="parent" />


                    <View
                        android:id="@+id/line"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_margin="10dp"
                        android:background="@color/line_color"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/document_txt" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/courier"
                        style="@style/style_9_bold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:text="@string/courier"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/line" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/name"
                        style="@style/style_15_bold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:maxWidth="150dp"
                        android:singleLine="true"
                        android:textColor="@color/black"
                        android:textSize="18sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/courier" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/status"
                        style="@style/style_13_bold"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginEnd="10dp"
                        android:gravity="center"
                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/line" />


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cl"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/name">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/pickup_txt"
                            style="@style/style_13_bold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="10dp"
                            android:text="@string/pick_up_details"
                            android:textColor="@color/black"
                            app:layout_constraintStart_toEndOf="@+id/relMain"
                            app:layout_constraintTop_toTopOf="parent" />


                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/pick_add"
                            style="@style/style_11_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:textColor="@color/black"
                            app:layout_constraintStart_toEndOf="@+id/relMain"
                            app:layout_constraintTop_toBottomOf="@+id/pickup_txt" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/pick_date_time"
                            style="@style/style_11_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:textColor="#979797"
                            android:textStyle="bold"
                            app:layout_constraintStart_toEndOf="@+id/relMain"
                            app:layout_constraintTop_toBottomOf="@+id/pick_add" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/pick_name"
                            style="@style/style_11_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="13dp"
                            android:textColor="@color/black"
                            app:layout_constraintStart_toEndOf="@+id/relMain"
                            app:layout_constraintTop_toBottomOf="@+id/pick_date_time" />


                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/pick_phone"
                            style="@style/style_11_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:textColor="@color/black"
                            app:layout_constraintStart_toEndOf="@+id/relMain"
                            app:layout_constraintTop_toBottomOf="@+id/pick_name" />


                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/pick_email"
                            style="@style/style_11_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:textColor="@color/black"
                            app:layout_constraintStart_toEndOf="@+id/relMain"
                            app:layout_constraintTop_toBottomOf="@+id/pick_phone" />

                        <View
                            android:id="@+id/line2"
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginStart="40dp"
                            android:layout_marginTop="20dp"
                            android:background="@color/line_color"
                            app:layout_constraintStart_toEndOf="@+id/relMain"
                            app:layout_constraintTop_toBottomOf="@+id/pick_email" />


                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/drop_txt"
                            style="@style/style_13_bold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="18dp"
                            android:text="@string/drop_off_details"
                            android:textColor="@color/black"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/relMain" />


                        <RelativeLayout
                            android:id="@+id/relMain"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="10dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"

                            >


                            <com.google.android.material.imageview.ShapeableImageView
                                android:id="@+id/imageViewStatus"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:src="@drawable/ic_pickup_icon" />

                            <com.google.android.material.imageview.ShapeableImageView
                                android:id="@+id/textViewJobStatus"
                                android:layout_width="1dp"
                                android:layout_height="130dp"
                                android:layout_below="@id/imageViewStatus"
                                android:layout_centerHorizontal="true"
                                android:src="@drawable/ic_broken_line" />


                            <com.google.android.material.imageview.ShapeableImageView
                                android:id="@+id/end_icon"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_below="@+id/textViewJobStatus"
                                android:src="@drawable/ic_drop_off_icon" />

                        </RelativeLayout>


                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/drop_add"
                        style="@style/style_11_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="40dp"
                        android:textColor="@color/black"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/cl" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/drop_date_time"
                        style="@style/style_11_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="40dp"
                        android:textColor="#979797"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/drop_add" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/drop_name"
                        style="@style/style_11_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="40dp"
                        android:layout_marginTop="13dp"
                        android:textColor="@color/black"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/drop_date_time" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/drop_phone"
                        style="@style/style_11_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="40dp"
                        android:textColor="@color/black"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/drop_name" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/drop_email"
                        style="@style/style_11_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="40dp"
                        android:layout_marginBottom="10dp"
                        android:textColor="@color/black"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/drop_phone" />


                </androidx.constraintlayout.widget.ConstraintLayout>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/package_txt"
                    style="@style/style_15_bold"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="20dp"
                    android:text="Package Image &amp; Signbature"
                    android:textColor="@color/black"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/card" />


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/card1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/white_background"
                    android:elevation="5dp"
                    app:layout_constraintTop_toBottomOf="@+id/package_txt">


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/pick_cl"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/drop_cl"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/pick_txt"
                            style="@style/style_13_bold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="10dp"
                            android:text="@string/pickup"
                            android:textColor="@color/black"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"

                            />

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/pick_doc"
                            android:layout_width="match_parent"
                            android:layout_height="60dp"
                            android:weightSum="2"
                            android:background="@drawable/border4side"
                            android:layout_marginStart="15dp"
                            android:layout_marginEnd="13dp"
                            android:orientation="horizontal"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/pick_txt">


                            <com.google.android.material.imageview.ShapeableImageView
                                android:id="@+id/pick_image"
                                android:layout_width="70dp"
                                android:layout_height="55dp"
                                android:layout_gravity="center"
                                android:layout_weight="0.5"
                                android:src="@mipmap/ic_box"
                                android:layout_marginEnd="5dp"
                                />


                            <View
                                android:id="@+id/line01"
                                android:layout_width="1dp"
                                android:layout_height="60dp"
                                android:background="@color/line_color" />


                            <com.google.android.material.imageview.ShapeableImageView
                                android:id="@+id/pick_signature"
                                android:layout_width="70dp"
                                android:layout_height="55dp"
                                android:layout_gravity="center"
                                android:layout_weight="0.5"
                                android:layout_marginStart="5dp"
                                android:src="@drawable/ic_blank_sing"
                             />

                        </androidx.appcompat.widget.LinearLayoutCompat>




                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/pick_time_header"
                            style="@style/style_11_bold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="10dp"
                            android:text="@string/timing"
                            android:textColor="@color/black"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/pick_doc" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/pick_time_req"
                            style="@style/style_9_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:layout_marginStart="10dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/pick_time_header" />


                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/pick_request"
                            style="@style/style_9_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:text="@string/requested"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/pick_time_req" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/pick_time_actual"
                            style="@style/style_9_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:textColor="@color/black"
                            android:layout_marginTop="15dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/pick_request" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/pick_actual"
                            style="@style/style_9_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:text="@string/actual"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/pick_time_actual" />

                    </androidx.constraintlayout.widget.ConstraintLayout>



                    <View
                        android:id="@+id/line0"
                        android:layout_width="1dp"
                        android:layout_height="200dp"
                        android:background="@color/line_color"
                        app:layout_constraintStart_toEndOf="@+id/pick_cl"
                        app:layout_constraintEnd_toStartOf="@+id/drop_cl"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>



                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/drop_cl"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/pick_cl"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginStart="3dp">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/drop_txt1"
                            style="@style/style_13_bold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:text="@string/Drop_off"
                            android:textColor="@color/black"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"

                            />


                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/drop_doc"
                            android:layout_width="match_parent"
                            android:layout_height="60dp"
                            android:weightSum="2"
                            android:background="@drawable/border4side"
                            android:layout_marginStart="15dp"
                            android:layout_marginEnd="15dp"
                            android:orientation="horizontal"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/drop_txt1">


                            <com.google.android.material.imageview.ShapeableImageView
                                android:id="@+id/drop_image"
                                android:layout_width="70dp"
                                android:layout_height="55dp"
                                android:layout_weight="0.5"
                                android:src="@mipmap/ic_box"
                                android:layout_marginEnd="5dp"/>


                            <View
                                android:id="@+id/line02"
                                android:layout_width="1dp"
                                android:layout_height="60dp"
                                android:background="@color/line_color"
                         />



                            <com.google.android.material.imageview.ShapeableImageView
                                android:id="@+id/drop_signature"
                                android:layout_width="70dp"
                                android:layout_height="55dp"
                                android:layout_gravity="center"
                                android:layout_weight="0.5"
                                android:src="@drawable/ic_blank_sing"
                                android:layout_marginStart="5dp"
                                />

                        </androidx.appcompat.widget.LinearLayoutCompat>




                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/drop_time_header"
                            style="@style/style_11_bold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="10dp"
                            android:text="@string/timing"
                            android:textColor="@color/black"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/drop_doc" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/drop_time_req"
                            style="@style/style_9_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:textColor="@color/black"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/drop_time_header" />


                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/drop_request"
                            style="@style/style_9_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:text="@string/requested"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/drop_time_req" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/drop_time_actual"
                            style="@style/style_9_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:textColor="@color/black"
                            android:layout_marginTop="15dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/drop_request" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/drop_actual"
                            style="@style/style_9_regular"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:text="@string/actual"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/drop_time_actual" />


                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>


                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/notes"
                    style="@style/style_15_bold"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:text="@string/notes"
                    android:textColor="@color/black"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/card1" />


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/card3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="20dp"
                    android:elevation="5dp"
                    android:background="@drawable/white_background"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/notes">

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/delivery_note_txt"
                        style="@style/style_13_bold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="15dp"
                        android:text="Delivery notes"
                        android:textColor="@color/black"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/delivery_note"
                        style="@style/style_9_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:text="No notes available"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/delivery_note_txt" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/package_note_txt"
                        style="@style/style_13_bold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:text="Package notes"
                        android:textColor="@color/black"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/delivery_note" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/package_note"
                        style="@style/style_9_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginBottom="15dp"
                        android:textColor="@color/black"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/package_note_txt" />


                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>


        </ScrollView>


    </androidx.constraintlayout.widget.ConstraintLayout>
